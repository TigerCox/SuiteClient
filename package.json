{
  "name": "vin-react-starter-kit",
  "version": "5.0.0",
  "description": "Starter kit for creating React apps at Vinsolutions",
  "scripts": {
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.11.1",
    "bootstrap": "3.3.6",
    "object-assign": "4.1.0",
    "react": "15.1.0",
    "react-bootstrap": "0.29.4",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.9.0",
    "browser-sync": "2.12.10",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "enzyme": "2.3.0",
    "eslint": "2.11.1",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-react": "5.1.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "glob": "7.0.3",
    "isparta": "4.0.0",
    "jsdom": "9.2.1",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "npm-install-webpack-plugin": "4.0.0",
    "npm-run-all": "2.1.1",
    "react-addons-test-utils": "15.1.0",
    "redux-immutable-state-invariant": "1.2.3",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  }
}
